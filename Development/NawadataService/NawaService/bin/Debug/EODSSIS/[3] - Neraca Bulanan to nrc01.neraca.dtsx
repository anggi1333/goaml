          SQLTask:SqlStatementSource="INSERT INTO DSV00P02.nrc01.neraca (periodeLaporan, idPelapor, cakupanData, nominalTotal, posLaporanPosisiKeuangan, periodeData, nominalRupiah, nominalValas)&#xA;SELECT TRY_CONVERT(VARCHAR(50), 'M') AS periodeLaporan, TRY_CONVERT(VARCHAR(50), (SELECT SettingValue from SystemParameter WHERE PK_SystemParameter_ID=9008) + Kode_Cabang_BI) AS idPelapor, TRY_CONVERT(VARCHAR(50), Cakupan_Data) AS cakupanData, TRY_CONVERT(NUMERIC(28,2), Jumlah) AS nominalTotal, TRY_CONVERT(VARCHAR(50), Pos_Neraca) AS posLaporanPosisiKeuangan, TRY_CONVERT(DATETIME, Report_Date) AS periodeData, TRY_CONVERT(NUMERIC(28,2), Rupiah) AS nominalRupiah, TRY_CONVERT(NUMERIC(28,2), Valas) AS nominalValas&#xA; FROM OneReportingDB.dbo.ANT_B_Neraca&#xA; &#xA; WHERE 1=1  AND DATEDIFF(DAY, Report_Date, ?) = 0 AND Kode_Cabang_BI = CASE ? WHEN 'All' THEN Kode_Cabang_BI ELSE ? END;&#xA;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
